<div id="index">
    <div class="header-main">
        <h1>Dashboard Football</h1>
        <h3>
            Quick access to football results, tables, top scorers and top assists
            from major leagues and competitions
        </h3>
    </div>
    <div class="items">
        <% items.forEach((item) => { %>
            <a class="item" href="/<%= item.code %>">
                <div class="item-icon icon-<%= item.code %>"></div>
                <div class="item-title">
                    <h2><%= item.name %></h2>
                </div>
            </a>
        <% }); %>
    </div>
</div>