<table class="table-data">
    <tr>
        <th title="Position">#</th>
        <th class="main-information" title="Player name">Name</th>
        <th class="main-information" title="Team">Team</th>
        <th title="Goals">Goals</th>
        <th title="Matches played">Pld</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.rank}}</td>
        <td class="main-information">{{this.name}}</td>
        <td class="main-information">{{this.team}}</td>
        <td>{{this.goals}}</td>
        <td>{{this.played}}</td>
    </tr>
    {{/each}}
</table>